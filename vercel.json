{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "functions": {
      "largeFunction": {
        "maxSize": "100mb",
        "src": "largeFunction.js"
      },
      "smallFunction": {
        "maxSize": "1mb",
        "src": "smallFunction.js"
      }
    }
  }